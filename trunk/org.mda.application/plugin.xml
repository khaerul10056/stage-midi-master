         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="MIDI Driven Application">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="appName"
               value="MIDI Driven Application">
         </property>
         <property
               name="applicationXMI"
               value="org.mda.commons.ui/Application.e4xmi">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/org.mda.commons.ui/css/bright-gradient.css">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://org.mda.application/org.mda.application.Application">
